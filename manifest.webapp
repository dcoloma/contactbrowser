{
  "name": "Concerts",
  "description": "Find which concerts are taking place close to where you are or a specific location.",
  "version": "0.01",
  "launch_path": "/concerts/index.html",
  "developer": {
    "name": "AppConcerts",
    "url": "https://twitter.com/appconcerts"
  },
  "default_locale": "es",
